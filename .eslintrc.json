{
  "extends": [
    "semistandard",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": false,
        "useTabs": true,
        "tabWidth": 2,
        "printWidth": 120
      }
    ]
  }
}